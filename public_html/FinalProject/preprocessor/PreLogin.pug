doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=100%, initial-scale=1.0')
  title Login
  link(rel='stylesheet' href='PreLogin.css')
.LoginPage
  .header
    .header_logo Lazebra
    .header_button.cursor
      img(src='./component/image/LoginSignIn.svg' alt='')
      |            Singin
  .container
    #ajax-output.container_title Deliver whatever you want
    .searchbar
      .searchbar_input
        img(src='./component/image/LoginSearch.svg' alt='')
        form(action='')
          input(type='text' name='place' placeholder='Enter delivery address')
      .searchbar_button.cursor Search
  .slider.hidden
    .slider_close.cursor
    .slider_title Sign in
    .slider_container
      .slider_content Welcome back!
      .slider_FBbutton.cursor
      |             or
      .slider_input
        form#ajax-form(action='#' method='get')
          input#email(type='text' name='email' placeholder='Email address or moblie number')
          input#password(type='text' name='password' placeholder='password')
      .slider_button.cursor Sign in
      .slider_footer
        | Haven&apos;t set up an account yet?
        a.cursor Register here
script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js')
script(src='https://code.jquery.com/ui/1.12.1/jquery-ui.js')
script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery-circle-progress/1.2.2/circle-progress.min.js')
script(src='Login.js')
